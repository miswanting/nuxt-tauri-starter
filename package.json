    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "bumpp": "^10.2.3",
    "commitizen": "^4.3.1",
    "conventional-changelog": "^7.1.1",
    "conventional-changelog-angular": "^8.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.35.0",
    "esno": "^4.8.0",
    "rimraf": "^6.0.1",
    "taze": "^19.6.0"
  }
}

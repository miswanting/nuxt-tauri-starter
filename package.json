{
  "name": "nuxt-tauri-starter",
  "version": "1.0.0",
  "packageManager": "pnpm@10.17.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "dev": "cd packages/backend && tauri dev",
    "build": "cd packages/backend && tauri build",
    "commit": "cz",
    "bump": "bumpp -r --no-commit --no-tag --no-push",
    "changelog": "conventional-changelog -p angular",
